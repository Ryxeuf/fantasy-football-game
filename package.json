{
  "name": "bloobowl",
  "private": true,
  "packageManager": "pnpm@9.7.0",
  "version": "0.1.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:web": "cd apps/web && npm run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier -w .",
    "typecheck": "turbo run typecheck --continue",
    "test": "turbo run test --continue",
    "test:integration": "npx vitest --config tests/integration/vitest.config.ts tests/integration",
    "changeset": "changeset",
    "changeset:version": "changeset version && pnpm install",
    "changeset:publish": "echo 'No registry publish (private)'; exit 0",
    "release": "semantic-release",
    "version:read": "node -p \"require('./package.json').version\""
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^23.0.2",
    "eslint": "^9.8.0",
    "prettier": "^3.3.3",
    "prisma": "^6.16.2",
    "tsx": "^4.16.2",
    "turbo": "2.1.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2"
  }
}
